{
    "name": "ticknical/postalian",
    "description": "Skeleton providing Japanese postal code API.",
    "keywords": ["Postal code", "郵便番号"],
    "license": "Apache-2.0",
    "type": "project",
    "authors": [
        {
            "name": "Tick",
            "email": "ticknical@perfectdark.jp"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
